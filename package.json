{
  "name": "multibot-web",
  "version": "1.0.0",
  "description": "MultiBot Web - Twitch bot management platform",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:integration": "node tests/scripts/test-obs-source-flow.js",
    "test:obs": "node tests/scripts/test-obs-source.js",
    "test:route": "node tests/scripts/test-audio-command-route.js",
    "test:commands": "node tests/scripts/test-all-commands.js",
    "test:commands:e2e": "node tests/scripts/test-audio-gif-end-to-end.js",
    "test:bits": "node tests/scripts/test-bits-simulation.js",
    "test:all": "npm run test:integration && npm test",
    "pre-deploy": "node scripts/pre-deployment-check.js"
  },
  "keywords": [
    "twitch",
    "bot",
    "streaming"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "animate.css": "^4.1.1",
    "axios": "^1.6.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "tmi.js": "^1.8.5",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
